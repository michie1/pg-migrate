{
  "name": "@urbica/pg-migrate",
  "version": "0.1.1",
  "description": "PostgreSQL migration tool",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.ru)",
  "license": "MIT",
  "main": "pg-migrate.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/urbica/pg-migrate.git"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "docs": "documentation build pg-migrate.js -g --markdown-toc false -f md -o API.md",
    "release": "standard-version",
    "test": "node test/index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bin": {
    "pg-migrate": "./index.js"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=7.6.0"
  },
  "keywords": [
    "postgres",
    "postgresql",
    "migration"
  ],
  "dependencies": {
    "documentation": "5.3.3",
    "dotenv": "4.0.0",
    "minimist": "1.2.0",
    "pg-monitor": "0.8.3",
    "pg-native": "2.2.0",
    "pg-promise": "7.2.1"
  },
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "standard-version": "4.2.0",
    "tape": "4.8.0"
  }
}
